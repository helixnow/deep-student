  "scripts": {
    "prebuild": "npm run version:generate",
    "build": "vite build",
    "build:all": "bash ./scripts/build_all.sh",
    "build:ios": "npm run icons:ios && npm run icons:ios:opaque && tauri ios build",
    "build:mac:oneclick": "bash ./scripts/build_mac.sh",
    "build:windows": "bash ./scripts/build_windows.sh",
    "version:generate": "node scripts/generate-version.mjs",
    "predev": "npm run version:generate",
    "dev": "vite --host --port 1422",
    "predev:tauri": "npm run version:generate",
    "pretauri": "npm run version:generate",
    "dev:tauri": "tauri dev",
    "predev:test": "npm run version:generate",
    "dev:test": "vite --host --port 1422 --open '/?test=true&debug-plugin=chat-test-runner&autorun=true'",
    "predev:test:multi-turn": "npm run version:generate",
    "dev:test:multi-turn": "vite --host --port 1422 --open '/?test=true&debug-plugin=chat-test-runner&autorun=true&autorun-case=basic-multi-turn'",
    "predev:test:editing": "npm run version:generate",
    "dev:test:editing": "vite --host --port 1422 --open '/?test=true&debug-plugin=chat-test-runner&autorun=true&autorun-case=basic-message-editing'",
    "predev:test:stop": "npm run version:generate",
    "dev:test:stop": "vite --host --port 1422 --open '/?test=true&debug-plugin=chat-test-runner&autorun=true&autorun-case=basic-stop-generation'",
    "icons": "tauri icon app-icon.png",
    "icons:ios": "tauri icon --output src-tauri/icons app-icon.png",
    "icons:ios:opaque": "node ./scripts/fix_ios_icons.mjs",
    "mcp:test": "node ./scripts/mcp-sse-test.mjs",
    "mcp:test:ws": "node ./scripts/mcp-ws-test.mjs",
    "prepreview": "npm run version:generate",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest run",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:unit": "vitest --config vitest.config.ts",
    "test:unit:ui": "vitest --config vitest.config.ts --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:components": "eslint src/ --rule 'no-restricted-imports: error' --rule 'ds-components/no-native-button: warn'",
    "lint:components:strict": "eslint src/ --rule 'no-restricted-imports: error' --rule 'ds-components/no-native-button: error'",
    "check:licenses": "node scripts/check-licenses.mjs",
    "test:lifecycle-gate": "node scripts/lifecycle-score-gate.mjs",
    "registry:sync": "node scripts/sync-model-registry.mjs"
